*
	margin 0
	padding 0
	outline none
body
	font-size: 18px
	font-family: G
	color #000
	background-color #fff
	line-height: 1.5
.wrapper
	overflow: hidden;
a
	color: #000
	&:focus
		outline: 0
		text-decoration none
	&:hover
		text-decoration none
h1,h2,h3,h4,h5,h6
	font-weight normal
	margin 0
button
	border none
section
header
footer
	width 100%
	display block
.video-wrapper
	margin 0 auto
	position relative
	width 100%
	top 0
	// z-index 60
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #000
		border-radius 0
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
		margin 0 auto
.button
	display block
	text-align center
	text-decoration none
	color: #000
	width: 100%
	padding: 35px 20px;
	box-shadow: inset 0px 1px 8.7px 1.3px rgba(255, 255, 255, 0.57),0px 1px 35px 0px rgba(158, 126, 192, 0.4);
	background-color: #bda2dc
	background-image: linear-gradient(to top, #bda2dc, #d0b3dd)
	border-radius: 5px;
	transition: all .3s ease;
	&:focus
		text-decoration none
		color #000
	&:hover
		text-decoration none
		color #000
		transform: translateY(3px)
		background-image: linear-gradient(to bottom, #bda2dc, #d0b3dd)
		box-shadow: inset 0px 1px 8.7px 1.3px rgba(255, 255, 255, 0.57),0px 1px 15px 0px rgba(158, 126, 192, 0.5);
	&-wrapper
		width: 100%;
		max-width: 384px;
		margin 0 auto
		position: relative;
		&:before
			content: ''
			display: block;
			position: absolute;
			width: 96%;
			margin: 0 auto;
			left: 0;
			right: 0;
			bottom: -3px;
			background-color: #bda0d9
			border-radius: 5px
.general-title
	text-align center
	font-size: 35px
	font-weight: bold
	margin-bottom: 50px
	text-transform uppercase
	&-left
		text-align: left;
.general-list
	li
		list-style: none
		padding-left: 34px
		margin-bottom: 20px
		position relative
		&:last-child
			margin-bottom: 0
		&:before
			content: ''
			display block
			position absolute
			top 0
			left 0
			width 26px
			height 26px
			border-radius: 50%
			border: 1px solid #c5a9dd
		&:after
			content: ''
			display block
			position absolute
			border-radius: 50%
			background-color: #c5a9dd
			box-shadow: inset 0px 1px 8.7px 1.3px rgba(255, 255, 255, 0.57);
			width: 14px;
			height: 14px;
			background-color: #bda2dc
			background-image: linear-gradient(to top, #bda2dc, #d0b3dd)
			left 6px
			top 6px
.general-text
	margin-bottom: 20px