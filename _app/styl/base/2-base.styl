.wrapper
	overflow: hidden;
.content
	position relative
a
	color: #4D4D4D
	&:focus
		outline: 0
		text-decoration none
	&:hover
		text-decoration none
h1,h2,h3,h4,h5,h6
	font-weight normal
	margin 0
button
	border none
section
header
footer
	width 100%
	display block
section
	width 100%
	max-width 1440px
	margin 0 auto
.video-wrapper
	margin 0 auto
	position relative
	width 100%
	top 0
	// z-index 60
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #000
		border-radius 0
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
		margin 0 auto
.button
	display inline-block
	text-align center
	text-decoration none
	font-size: 20px
	color: #fff
	width: auto
	padding: 25px 50px;
	background-color: mP
	border-radius: 50px;
	transition: all .6s ease;
	position relative
	z-index 999
	@media screen and (max-width 560px)
		font-size: 18px
		padding 13px 35px
	&:focus
		text-decoration none
		color #fff
	&:hover
		text-decoration none
		color #000
		transform: translateY(-3px)
		background-color #F0DFFC
.general-title
	font-size: 42px
	font-weight: bold
	margin-bottom: 50px
	color mP
	line-height: 105%
	@media screen and (max-width 991px)
		font-size: 32px
		margin-bottom: 40px
	@media screen and (max-width 560px)
		font-size: 26px
		margin-bottom: 30px
.general-list
	li
		list-style: none
		padding-left: 30px
		margin-bottom: 30px
		position relative
		@media screen and (max-width 560px)
			margin-bottom: 20px
		&:last-child
			margin-bottom: 0
		span
			font-weight: bold
		&:before
			content: ''
			display block
			position absolute
			top 0
			left 0
			width 18px
			height 18px
			border-radius: 50%
			background-color: mP
		&:after
			content: ''
			display block
			position absolute
			top 6px
			left 5px
			width 9px
			height: 5px
			border-bottom: 2px solid #fff
			border-left: 2px solid #fff
			transform: rotate(-39deg)
.general-text
	margin-bottom: 20px
.general-img
	position absolute